{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "pretty": true,
        "sourceMap": true,
        "target": "es6",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "outDir": "./dist"
    },
    "include": [
        "app.ts",
        "config.json",
        "libs/*.ts",
        "commands/*.ts",
        "commands/**/*.ts",
        "config/*.ts"
    ]
}